    "    \n",
    "    def calculate_verified_opportunity_score(self, category, origin_country):\n",
    "        \"\"\"검증된 데이터 기반 기회 점수 계산\"\"\"\n",
    "        \n",
    "        # 통계청 온라인쇼핑 성장률 기반 수요 점수\n",
    "        online_growth = 11.2  # 2024년 평균 성장률\n",
    "        if online_growth > 10:\n",
    "            demand_score = 85\n",
    "        elif online_growth > 5:\n",
    "            demand_score = 65\n",
    "        else:\n",
    "            demand_score = 45\n",
    "        \n",
    "        # 관세청 데이터 기반 비용 점수\n",
    "        customs_data = self.get_customs_import_categories()\n",
    "        category_info = customs_data[customs_data['category'] == category]\n",
    "        \n",
    "        if not category_info.empty:\n",
    "            duty_rate = category_info.iloc[0]['avg_duty_rate_percent']\n",
    "            if duty_rate < 5:\n",
    "                cost_score = 90\n",
    "            elif duty_rate < 10:\n",
    "                cost_score = 70\n",
    "            elif duty_rate < 15:\n",
    "                cost_score = 50\n",
    "            else:\n",
    "                cost_score = 30\n",
    "        else:\n",
    "            cost_score = 50  # 기본값\n",
    "        \n",
    "        # K-stat 데이터 기반 물류 점수\n",
    "        trade_data = self.get_kstat_trade_data()\n",
    "        country_info = trade_data[trade_data['country'] == origin_country]\n",
    "        \n",
    "        if not country_info.empty:\n",
    "            logistics_score = country_info.iloc[0]['logistics_cost_score'] * 10\n",
    "        else:\n",
    "            logistics_score = 50  # 기본값\n",
    "        \n",
    "        # 가중평균 계산\n",
    "        weights = {\n",
    "            'demand': 0.4,    # 시장 수요 (통계청 데이터)\n",
    "            'cost': 0.35,     # 관세 및 비용 (관세청 데이터)\n",
    "            'logistics': 0.25  # 물류 효율성 (K-stat 데이터)\n",
    "        }\n",
    "        \n",
    "        total_score = (\n",
    "            demand_score * weights['demand'] +\n",
    "            cost_score * weights['cost'] +\n",
    "            logistics_score * weights['logistics']\n",
    "        )\n",
    "        \n",
    "        return {\n",
    "            'total_score': round(total_score, 1),\n",
    "            'demand_score': demand_score,\n",
    "            'cost_score': cost_score,\n",
    "            'logistics_score': logistics_score,\n",
    "            'recommendation': self._get_verified_recommendation(total_score)\n",
    "        }\n",
    "    \n",
    "    def _get_verified_recommendation(self, score):\n",
    "        \"\"\"점수 기반 검증된 추천\"\"\"\n",
    "        if score >= 80:\n",
    "            return {'level': 'HIGH', 'action': '적극 진출 권장', 'color': 'green'}\n",
    "        elif score >= 65:\n",
    "            return {'level': 'MEDIUM', 'action': '진출 검토', 'color': 'blue'}\n",
    "        elif score >= 50:\n",
    "            return {'level': 'LOW', 'action': '신중한 검토 필요', 'color': 'orange'}\n",
    "        else:\n",
    "            return {'level': 'AVOID', 'action': '진출 비추천', 'color': 'red'}\n",
    "\n",
