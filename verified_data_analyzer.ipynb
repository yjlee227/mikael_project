    "            },\n",
    "            '미국': {\n",
    "                'import_volume_billion_usd': 85.2,\n",
    "                'export_volume_billion_usd': 95.6,\n",
    "                'trade_balance': 10.4,\n",
    "                'main_import_items': ['전자제품', '의류', '화장품'],\n",
    "                'currency_stability': '높음',\n",
    "                'logistics_cost_score': 6.5\n",
    "            },\n",
    "            '일본': {\n",
    "                'import_volume_billion_usd': 48.9,\n",
    "                'export_volume_billion_usd': 29.8,\n",
    "                'trade_balance': -19.1,\n",
    "                'main_import_items': ['전자제품', '화장품', '생활용품'],\n",
    "                'currency_stability': '높음',\n",
    "                'logistics_cost_score': 8.0\n",
